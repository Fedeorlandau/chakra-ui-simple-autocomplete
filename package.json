{
  "name": "chakra-ui-simple-autocomplete",
  "author": "Federico Orlandau",
  "description": "A simple autocomplete input built with Chakra UI",
  "version": "2.2.0",
  "license": "MIT",
  "keywords": [
    "chakra",
    "chakra-ui",
    "autocomplete",
    "input",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Fedeorlandau/chakra-ui-simple-autocomplete"
  },
  "scripts": {
    "dev": "concurrently \"yarn build --watch\" \"yarn storybook\"",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"{src,tests}/**/*.+(ts|js|tsx)\"",
    "lint:fix": "yarn lint --fix",
    "test": "jest --coverage",
    "test:ci": "yarn test --ci",
    "test:watch": "jest --watch",
    "prepare": "husky install",
    "commit": "cz",
    "storybook": "start-storybook -p 6006 --quiet",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "release": "release"
  },
  "tsup": {
    "entry": [
      "src/index.tsx"
    ],
    "treeshake": true,
    "sourcemap": true,
    "minify": true,
    "clean": true,
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react"
    ]
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "lint-staged": {
    "./{src,tests}/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@ryansonshine/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.18.5",
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@ryansonshine/commitizen": "4.2.8",
    "@ryansonshine/cz-conventional-changelog": "3.3.4",
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
    "@storybook/addon-links": "6.5.9",
    "@storybook/builder-webpack5": "6.5.9",
    "@storybook/manager-webpack5": "6.5.9",
    "@storybook/react": "6.5.9",
    "@storybook/storybook-deployer": "2.8.11",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/jest": "28.1.2",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.28.0",
    "@typescript-eslint/parser": "5.28.0",
    "babel-loader": "8.2.5",
    "concurrently": "7.2.2",
    "eslint": "8.18.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-storybook": "0.5.12",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "8.0.1",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.3",
    "lint-staged": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-test-renderer": "18.2.0",
    "release": "6.3.1",
    "ts-jest": "28.0.5",
    "tsup": "6.1.2",
    "typescript": "4.7.4"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "resolutions": {
    "glob-parent": "5.1.2",
    "debug": "2.6.9",
    "node-fetch": "2.6.7",
    "trim": "0.0.3",
    "trim-newlines": "3.0.1"
  },
  "dependencies": {
    "@chakra-ui/icon": "3.0.4",
    "@chakra-ui/icons": "2.0.4",
    "@chakra-ui/react": "2.2.4",
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "framer-motion": "6.5.1",
    "match-sorter": "6.3.1"
  }
}
